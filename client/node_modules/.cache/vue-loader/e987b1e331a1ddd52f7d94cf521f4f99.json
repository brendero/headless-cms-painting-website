../modal/Modal.vue';\n\nexport default {\n  name: 'GridItem',\n  components: {\n    Modal\n  },\n  props: {\n    item: Object\n  },\n  data: () => {\n    return {\n      showModal: false,\n      renderedImage: null,\n      loading: true\n    }\n  },\n  mounted() {\n    axios\n      .get(`${process.env.VUE_APP_API_URL}/media/${this.item.image}`)\n      .then(({data}) => this.renderedImage = data.guid.rendered)\n      .finally(() => this.loading = false)\n  }\n}\n</script>\n\n<style>\n.grid-item {\n  margin: 0;\n}\n.grid-item:hover {\n  cursor: pointer;\n}\n.grid-item > img {\n  width: 100%;\n  height: auto;\n  object-fit: cover;\n}\n.grid-item > img:hover {\n  opacity: 0.7;\n}\n.skeleton {\n  background: linear-gradient(-45deg, rgba(211, 211, 211, 1), rgba(211, 211, 211, 0.2));\n\tbackground-size: 400% 400%;\n  height: 400px;\n  width: 100%;\n\tanimation: gradient 1.5s ease infinite;\n}\n@keyframes gradient {\n\t0% {\n\t\tbackground-position: 0% 50%;\n\t}\n\t50% {\n\t\tbackground-position: 100% 50%;\n\t}\n\t100% {\n\t\tbackground-position: 0% 50%;\n\t}\n}\n</style>"]}]}